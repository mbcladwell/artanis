()
`((xhtml-1.0-strict)
  (html
   (head
    (title ,($ 'title))
    (meta (@ (http-equiv "Content-Style-Type") (content "text/css")))
    (meta (@ (http-equiv "Content-Type") (content "text/html; charset=utf-8")))
    (meta (@ (name "description") (content ,description-contents)))
    (meta (@ (name "keywords") (content ,keyword-contents)))
    (meta (@ (name "flattr:id") (content "kl95gr")))
    (link (@ (href "css/highlight.css") (rel "stylesheet") (type "text/css")))
    (link (@ (href "css/common.css") (rel "stylesheet") (type "text/css"))))     
    (link (@ (rel "icon") (type "image/ico") (href "favicon.ico")))
   (body
    (div (@ (id "side"))
     (div (@ (class "wrap"))
      (h1 "Artanis home")
      (h2 (@ (id "slogan1")) "A fast monolithic web-framework")
      (h2 (@ (id "slogan2")) "of " (a (@ (id "slogan2") (href "scheme.html")) "Scheme"))
      (ul (@ (id "links"))
       (li (a (@ (href "index.html") (class "active")) "Home"))
       (li (a (@ (href "about.html")) "About"))
       (li (a (@ (href "http://gnu.org/software/artanis/manual")) "Documents"))
       (li (a (@ (href "http://ftp.gnu.org/gnu/artanis/")) "Download"))
       (li (@ (class "last")) (a (@ (href "#")) "Donate")))

    (p (@ (id "honor")) "2013 Lisp in summer projects")
    (p (a (@ (id "honor") (href "http://LispInSummerProjects.org/awesome-projects"))
           "certificated awesome project"))
    (h3 (@ (id "the-author")) "Authored by " 
       (a (@ (id "the-author") (href "http://nalaginrut.com")) "NalaGinrut"))
    (a (@ (href "http://gnu.org/s/artanis"))
       (img (@ (src "http://www.gnu.org/graphics/heckert_gnu.small.png")
            (height "80") (width "80"))))
    (a (@ (href "http://szdiy.org"))
       (img (@ (src "http://wiki.szdiy.org/skins/common/images/szdiy.png")
               (height "100") (width "100"))))
    (br)))


    (div (@ (id "main") (class "home"))
     (div (@ (class "wrap")) 
          (pre (code (inject ,contents)))

          )

     (p (@ (class "foot up"))
	(a (@ (href "http://gnu.org/software/artanis")) "Copyright  2015 Free Software Foundation, Inc.")
	" Source: "
        (a (@ (href "https://gitlab.com/NalaGinrut/artanis")) "Artanis ")
        "powered by " (a (@ (href "http://www.gnu.org/software/guile/")) "GNU Guile ")
        "This page is Powered by " (a (@ (href "http://wiki.call-cc.org/eggref/4/hyde")) "Hyde of Chicken Scheme")))
    ;; fork me on gitlab
    (a (@ (id "forkme") (href "https://gitlab.com/NalaGinrut/artanis")) "Fork me on GitLab")


    )))
